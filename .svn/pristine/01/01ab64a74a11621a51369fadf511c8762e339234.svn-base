public class TbTenantUser extends BaseEntity implements Serializable {
	
	private static final long serialVersionUID = 1L;
	
	/**租户id*/
	private String zhId;
	/**属地区域id*/
	private String adId;
	/**租户类型id*/
	private String zhTypeId;
	/**租户名称*/
	private String zhName;
	/**租户简称*/
	private String zhShortName;
	/**租户编号，唯一的*/
	private String zhCode;
	
	
	/**
	 *方法: 取得String
	 *@return: String  租户id
	 */
	public String getZhId(){
		return this.zhId;
	}

	/**
	 *方法: 设置String
	 *@param: String  租户id
	 */
	public void setZhId(String zhId){
		this.zhId = zhId;
	}
	
	/**
	 *方法: 取得String
	 *@return: String  属地区域id
	 */
	public String getAdId(){
		return this.adId;
	}

	/**
	 *方法: 设置String
	 *@param: String  属地区域id
	 */
	public void setAdId(String adId){
		this.adId = adId;
	}
	
	/**
	 *方法: 取得String
	 *@return: String  租户类型id
	 */
	public String getZhTypeId(){
		return this.zhTypeId;
	}

	/**
	 *方法: 设置String
	 *@param: String  租户类型id
	 */
	public void setZhTypeId(String zhTypeId){
		this.zhTypeId = zhTypeId;
	}
	
	/**
	 *方法: 取得String
	 *@return: String  租户名称
	 */
	public String getZhName(){
		return this.zhName;
	}

	/**
	 *方法: 设置String
	 *@param: String  租户名称
	 */
	public void setZhName(String zhName){
		this.zhName = zhName;
	}
	
	/**
	 *方法: 取得String
	 *@return: String  租户简称
	 */
	public String getZhShortName(){
		return this.zhShortName;
	}

	/**
	 *方法: 设置String
	 *@param: String  租户简称
	 */
	public void setZhShortName(String zhShortName){
		this.zhShortName = zhShortName;
	}
	
	/**
	 *方法: 取得String
	 *@return: String  租户编号，唯一的
	 */
	public String getZhCode(){
		return this.zhCode;
	}

	/**
	 *方法: 设置String
	 *@param: String  租户编号，唯一的
	 */
	public void setZhCode(String zhCode){
		this.zhCode = zhCode;
	}
	
	
}
