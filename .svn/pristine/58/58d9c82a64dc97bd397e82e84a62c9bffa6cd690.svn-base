<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.dhcc.jn.tenant.dao.TbOrgDao">

	<parameterMap id="parameterMap" type="com.dhcc.jn.tenant.entity.TbOrg"></parameterMap>
	<resultMap id="resultMap" type="com.dhcc.jn.tenant.entity.TbOrg">
		<result property="orgId" column="org_id"/>
		<result property="zhId" column="zh_id"/>
		<result property="orgCode" column="org_code"/>
		<result property="orgName" column="org_name"/>
		<result property="parentOrgId" column="parent_org_id"/>
		<result property="orgType" column="org_type"/>
		<result property="orgOrder" column="org_order"/>
		<result property="createId" column="create_id"/>
		<result property="createName" column="create_name"/>
		<result property="createDate" column="create_date"/>
		<result property="updateId" column="update_id"/>
		<result property="updateName" column="update_name"/>
		<result property="updateDate" column="update_date"/>
	</resultMap>
	
	<sql id="columnSql">
		<trim suffixOverrides=",">
			t.org_id,
			t.zh_id,
			t.org_code,
			t.org_name,
			t.parent_org_id,
			t.org_type,
			t.org_order,
			t.create_id,
			t.create_name,
			t.create_date,
			t.update_id,
			t.update_name,
			t.update_date,
	 	</trim>
	</sql>
	
	<select id="find" parameterMap="parameterMap" resultMap="resultMap">
		select
			<include refid="columnSql"></include>
		from tb_org t
		<where>
			<if test="orgId != null and orgId != ''">
				and t.org_id = #{orgId}			
			</if>
			<if test="zhId != null and zhId != ''">
				and t.zh_id = #{zhId}			
			</if>
			<if test="orgCode != null and orgCode != ''">
				and t.org_code = #{orgCode}			
			</if>
			<if test="orgName != null and orgName != ''">
				and t.org_name = #{orgName}			
			</if>
			<if test="parentOrgId != null and parentOrgId != ''">
				and t.parent_org_id = #{parentOrgId}			
			</if>
			<if test="orgType != null and orgType != ''">
				and t.org_type = #{orgType}			
			</if>
		</where>
	</select>
	
	<select id="getById" parameterType="java.lang.String" resultMap="resultMap">
		select
			<include refid="columnSql"></include>
		from tb_org t
		where t.org_id = #{value}
	</select>
	
	<insert id="add" parameterMap="parameterMap">
		insert into tb_org
		<trim prefix="(" suffix=")" suffixOverrides=",">
			org_id,
			zh_id,
			org_code,
			org_name,
			parent_org_id,
			org_type,
			org_order,
			create_id,
			create_name,
			create_date,
			update_id,
			update_name,
			update_date,
		</trim>
		<trim prefix="values (" suffix=")" suffixOverrides=",">
			#{orgId},
			#{zhId},
			#{orgCode},
			#{orgName},
			#{parentOrgId},
			#{orgType},
			#{orgOrder},
			#{createId},
			#{createName},
			#{createDate},
			#{updateId},
			#{updateName},
			#{updateDate},
		</trim>
	</insert>
	
	<insert id="batchAdd" parameterType="java.util.List">
		insert into tb_org
		<trim prefix="(" suffix=")" suffixOverrides=",">
			org_id,
			zh_id,
			org_code,
			org_name,
			parent_org_id,
			org_type,
			org_order,
			create_id,
			create_name,
			create_date,
			update_id,
			update_name,
			update_date,
		</trim>
		values
		<foreach collection="list" item="item" separator=",">
			<trim prefix="(" suffix=")" suffixOverrides=",">
				#{item.orgId},
				#{item.zhId},
				#{item.orgCode},
				#{item.orgName},
				#{item.parentOrgId},
				#{item.orgType},
				#{item.orgOrder},
				#{item.createId},
				#{item.createName},
				#{item.createDate},
				#{item.updateId},
				#{item.updateName},
				#{item.updateDate},
			</trim>
		</foreach>
	</insert>
	
	<update id="update" parameterMap="parameterMap">
		update tb_org
		<set>
			zh_id = #{zhId},
			org_code = #{orgCode},
			org_name = #{orgName},
			parent_org_id = #{parentOrgId},
			org_type = #{orgType},
			org_order = #{orgOrder},
			create_id = #{createId},
			create_name = #{createName},
			create_date = #{createDate},
			update_id = #{updateId},
			update_name = #{updateName},
			update_date = #{updateDate},
		</set>
		where org_id = #{orgId}
	</update>
	
	<update id="updateNotNull" parameterMap="parameterMap">
		update tb_org
		<set>
			<if test="zhId != null">
			zh_id = #{zhId},
			</if>
			<if test="orgCode != null">
			org_code = #{orgCode},
			</if>
			<if test="orgName != null">
			org_name = #{orgName},
			</if>
			<if test="parentOrgId != null">
			parent_org_id = #{parentOrgId},
			</if>
			<if test="orgType != null">
			org_type = #{orgType},
			</if>
			<if test="orgOrder != null">
			org_order = #{orgOrder},
			</if>
			<if test="createId != null">
			create_id = #{createId},
			</if>
			<if test="createName != null">
			create_name = #{createName},
			</if>
			<if test="createDate != null">
			create_date = #{createDate},
			</if>
			<if test="updateId != null">
			update_id = #{updateId},
			</if>
			<if test="updateName != null">
			update_name = #{updateName},
			</if>
			<if test="updateDate != null">
			update_date = #{updateDate},
			</if>
		</set>
		where org_id = #{orgId}
	</update>
	
	<delete id="delete" parameterType="java.lang.String">
		delete from tb_org where org_id = #{value}
	</delete>
	
	<delete id="batchDelete" parameterType="java.util.List">
		delete from tb_org where org_id in
		<foreach collection="list" item="item" open="(" close=")" separator=",">
			#{item}
		</foreach>
	</delete>
	
	<!-- 获取所有组织 -->
	<select id="findAll" resultMap="resultMap">
		SELECT 
			<include refid="columnSql"/>
		from tb_org t
		where 
		<where>
			<if test="parentOrgId != null and parentOrgId != ''">
				and t.parent_org_id = #{parentOrgId}	
			</if>
			<if test="parentOrgId == null and parentOrgId == ''">
				and t.parent_org_id = '402884696d6b4190016d6c6078d90009'
			</if>
		</where>
	</select>
	
	<select id="findBySearch" resultMap="resultMap">
		SELECT 
			<include refid="columnSql"/>
		from tb_org t
		where t.parent_org_id = #{parentOrgId}
	</select>
	
	<select id="getTbOrgTree" resultMap="resultMap">
		SELECT 
			<include refid="columnSql"/>
		from tb_org t
		where t.parent_org_id is null or t.parent_org_id = ''
	</select>
	
	
	<!-- 获取租户下的所有的组织 -->
	<select id="findOrgByzhId" resultMap="resultMap">
		SELECT 
			<include refid="columnSql"/>
		from tb_org t
		where t.zh_id=#{zhId}
	</select>
	<!-- （组织名称（模糊查询）、级别（精确查询）；） -->
	<select id="getByOrgNameAndOrgType" resultMap="resultMap">
		SELECT 
			<include refid="columnSql"/>
		from tb_org t
		<where>
			<if test="orgName != null and orgName != ''">
				and t.org_name like '%${orgName}%'	
			</if>
			<if test="orgType != null and orgType != ''">
				and t.org_type = #{orgType}	
			</if>
		</where>
	</select>
	<!-- 组织名唯一校验 -->
	<select id="getByOrgName" resultMap="resultMap">
		SELECT 
			<include refid="columnSql"/>
		from tb_org t
		where t.org_name=#{orgName} and t.zh_id=#{zhId}
	</select>
</mapper>